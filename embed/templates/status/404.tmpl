                    <div style="margin-top: 30px;">
                        <a href="javascript:history.back()" class="layui-btn layui-btn-primary">
                            <i class="layui-icon layui-icon-return"></i> 返回上一页
                        </a>
                        <a href="/" class="layui-btn layui-btn-normal">
                            <i class="layui-icon layui-icon-home"></i> 返回首页
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
